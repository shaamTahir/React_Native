      "package": "com.mrshaamt.easy_peezy_shop",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptiveIcon.png",
        "backgroundColor": "#171717"
      },
      "permissions": []
    }
