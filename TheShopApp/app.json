{
  "expo": {
    "name": "TheShopApp",
    "slug": "easy-peezy-shop",
    "description": "A fully responsive shopping app in which user is greet with a login screen, user can login or create a new account. After login, user can shop through or can add its own items. \nDummy E-Mail: Tes@gmail.com\n Password: shaamt",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
